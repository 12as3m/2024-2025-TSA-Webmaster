    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>webmaster thing 2</RootNamespace>
    <AssemblyName>$(AssemblyName.Replace(' ', '_'))</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
